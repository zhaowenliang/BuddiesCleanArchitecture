<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="cc.buddies.cleanarch.fragment.HomeFragment"
        android:label="@string/main_navigation_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_square"
        android:name="cc.buddies.cleanarch.fragment.SquareFragment"
        android:label="@string/main_navigation_square"
        tools:layout="@layout/fragment_square" />

    <fragment
        android:id="@+id/navigation_message"
        android:name="cc.buddies.cleanarch.fragment.MessageFragment"
        android:label="@string/main_navigation_message"
        tools:layout="@layout/fragment_message" />

    <fragment
        android:id="@+id/navigation_mine"
        android:name="cc.buddies.cleanarch.fragment.MineFragment"
        android:label="@string/main_navigation_mine"
        tools:layout="@layout/fragment_mine" />

</navigation>